extends layout

block content
  .w3-card-4
    header.w3-container.w3-blue
      h2 Contrato #{contrato._id}
    
    .w3-container
      table.w3-table-all
        each val, key in contrato
          tr
            th= key
            td= val
    
    footer.w3-container.w3-blue
      a.w3-button(href='/') Voltar